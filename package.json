{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "Cypress automation project",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "headtest": "npx cypress run --headed",
    "record": "npx cypress run --record -- key ",
    "chromeTest": "npx run test --browser chrome",
    "Greenkartest": "npx cypress run --spec \"cypress/integration/GreenKart/*\""
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/results.json"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.7",
    "cypress": "^13.11.0",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "@cypress/browserify-preprocessor": "latest",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "typescript": "^5.4.5",
    "neat-csv":"5.1.0"
  }
}
